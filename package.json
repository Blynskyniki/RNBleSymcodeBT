{
  "name": "rn-symcode-bt",
  "version": "0.1.21",
  "description": "React native library from Symcode barcodes scaner",
  "main": "dist/src/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test": "jest",
    "typescript": "tsc",
    "build": "rm -rf ./dist && npm run typescript",
    "vUp": "npm --no-git-tag-version version patch",
    "prepare": "npm run build && npm run vUp",
    "prepublish": "npm run prepare"
  },
  "keywords": [
    "symcode",
    "react-native",
    "android"
  ],
  "repository": "https://github.com/Blynskyniki/RNBleSymcodeBT",
  "author": "Blynskyniki <blynskyniki@gmail.com> (https://github.com/Blynskyniki)",
  "contributors": [
    "Petrova Ksenia <ksututa@gmail.com> (https://github.com/petrovakd)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Blynskyniki/RNBleSymcodeBT/issues"
  },
  "homepage": "https://github.com/Blynskyniki/RNBleSymcodeBT#readme",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.6.8",
    "jest": "^24.8.0",
    "react-native": "^0.55.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  }
}
