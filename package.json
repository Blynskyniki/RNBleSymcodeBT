{
  "name": "rn-symcode-bt",
  "version": "0.1.3",
  "description": "React native library from Symcode barcodes scaner",
  "main": "dist/src/index.ts",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "build": "rm -rf ./dist &&  tsc",
    "pub": "npm run build && npm publish --registry https://npm.kari.com"
