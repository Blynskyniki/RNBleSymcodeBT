  "keywords": [
    "symcode",
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/Blynskyniki/RNBleSymcodeBT",
  "author": "Blynskyniki <blynskyniki@gmail.com> (https://github.com/Blynskyniki)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Blynskyniki/RNBleSymcodeBT/issues"
  },
  "homepage": "https://github.com/Blynskyniki/RNBleSymcodeBT#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.6.8",
    "jest": "^24.8.0",
    "react-native": "^0.55.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  }
